{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@5.0.6",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "vis@4.21.0",
      "destination": "wwwroot/lib/vis/",
      "files": [
        "vis.min.js",
        "vis-network.min.js",
        "vis-network.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap@5.1.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    }
  ]
}